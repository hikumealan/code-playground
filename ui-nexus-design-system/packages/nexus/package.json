{
  "name": "@nexus/core",
  "version": "2.0.0",
  "description": "Stencil Component Starter",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/nexus/nexus.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "lint": "eslint ./src --ext ts,tsx --ignore-path ../../.gitignore",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --coverage --reporters=default --reporters=jest-junit --json --outputFile=.jest-test-results.json",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "stencil": "stencil"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/core": "2.16.1",
    "@stencil/react-output-target": "^0.2.1",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "27.4.0",
    "@types/puppeteer": "5.4.4",
    "babel-loader": "^8.2.5",
    "fs-extra": "9.0.1",
    "jest": "^27.0.0",
    "jest-cli": "^27.0.0",
    "lit-html": "^1.3.0",
    "jest-config": "^27.0.0",
    "path": "0.12.7"
  },
  "license": "MIT",
  "dependencies": {
    "hammerjs": "2.0.8",
    "moment": "^2.29.4",
    "tocca": "2.0.9",
    "zingtouch": "1.0.6",
    "d3": "^5.12.0",
    "d3-array": "^2.3.1",
    "d3-axis": "^1.0.12",
    "d3-collection": "^1.0.7",
    "d3-scale": "^3.1.0",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.5",
    "d3-transition": "^1.2.0",
    "kebab-case": "^1.0.0"
  }
}